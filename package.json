{
  "name": "wizard",
  "version": "0.0.1",
  "author": "An Duong <an.duongx@gmail.com>",
  "description": "Abstract MERN build process",
  "homepage": "https://github.com/anduong96/Wizard#readme",
  "license": "MIT",
  "preferGlobal": true,
  "scripts": {
    "start": "node bin/wizard.js dev",
    "build": "babel src --out-dir dist",
    "build:watch": "babel --watch src --out-dir dist"
  },
  "engines": {
    "node": ">=8"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "8"
          }
        }
      ]
    ]
  },
  "bin": {
    "wizard": "bin/wizard"
  },
  "keywords": [
    "cli",
    "mern",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://anduong96@github.com/anduong96/Wizard.git"
  },
  "bugs": {
    "url": "https://github.com/anduong96/Wizard/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "ghooks": "^2.0.4",
    "moment-locales-webpack-plugin": "^1.0.7",
    "uglifyjs-webpack-plugin": "^1.0.0",
    "vinyl-named": "^1.1.0",
    "webpack": "^3.12.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-stream": "^4.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "console.table": "^0.10.0",
    "gulp": "^3.9.1",
    "gulp-git": "^2.9.0",
    "gulp-gzip": "^1.4.2",
    "gulp-minify": "^3.1.0",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "minimist": "^1.2.0"
  }
}
